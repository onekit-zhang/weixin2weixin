<import src="../../../common/head.wxml"/>
<import src="../../../common/foot.wxml"/>

<view class="container">
  <template is="head" data="{{title: 'image'}}">
</template>

  <view class="page-body">
    <view class="page-section page-section-gap">
      <view class="page-section-title">Local Image</view>
      <view class="page-section-ctn">
        <image class="image" src="../../resources/pic/1.jpg"></image>
      </view>
    </view>
    <view class="page-section page-section-gap">
      <view class="page-section-title">Internet Image</view>
      <view class="page-section-ctn">
        <image class="image" src="{{imageUrl}}"></image>
      </view>
    </view>
     <view class="page-section page-section-gap">
      <view class="page-section-title">Webp Image</view>
      <view class="page-section-ctn">
        <image class="image" webp src="{{webpImageUrl}}" mode="aspectFit"></image>
      </view>
    </view>
  </view>

  <template is="foot">
</template>
</view>